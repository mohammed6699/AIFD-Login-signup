# Supabase Configuration
# Copy this file to .env.local and replace the values with your actual Supabase credentials

# Your Supabase project URL (found in your Supabase dashboard)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url_here

# Your Supabase anon/public key (found in your Supabase dashboard)
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Your Supabase service role key (found in your Supabase dashboard)
# This is used for server-side operations
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Optional: Your site URL for sharing polls
NEXT_PUBLIC_SITE_URL=http://localhost:3000
